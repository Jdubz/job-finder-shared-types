{
  "name": "@jsdubzw/job-finder-shared-types",
  "version": "1.1.1",
  "description": "Shared TypeScript type definitions for job-finder (Python) and portfolio (TypeScript) projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepare": "husky",
    "prevent-no-verify": "./scripts/setup-no-verify-prevention.sh",
    "activate-git-prevention": "source scripts/activate-git-prevention.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jdubz/job-finder-shared-types.git"
  },
  "keywords": [
    "typescript",
    "types",
    "job-finder",
    "portfolio",
    "queue",
    "firestore"
  ],
  "author": "Josh Wentworth",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.7",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
